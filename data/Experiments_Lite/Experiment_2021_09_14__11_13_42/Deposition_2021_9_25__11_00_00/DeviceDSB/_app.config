<?xml version="1.0" encoding="utf-8"?>
<!-- This file supports handling the differences between the three GC PC's -->
<!-- The xml syntax consistency is verified by Visual Studio when compiling -->
<!-- This file is used by GC_InfrastructureChecker, following switch on PC names is implemented in SettingsLoad -->
<!-- Differenze nel software e nei files di config: gestite con GitHub -->
<!-- Differenze nel DB: DB (ogni PC ha il suo), DB names, myini, DB versions (Symbio ha tabelle in piu' per supportare CN com e pirometro com) -->
<!-- Differenze nella camera: Symbio leggermente diversa da Laserdyne, e quindi config files non compatibili -->
<!-- Differenze nella struttura della dir a supporto del DSB in D:\_GC_DataSetBuilder -->
<!--   viene creata se non esiste (prima volta che DSB gira), e contiene immagini acquisite, blobs, config files ecc) -->
<!-- Differenza di OS: win10 on Laserdyne vs win7 on Symbio (mounting a win7-only compliant high speed acquisition card) -->
<!-- Differenze nei drivers ed applicativi da installare (cfr. lunga lista sotto switchdrive PCVision/PC.docx): -->
<configuration>
  <appSettings>
    <!-- GCWS Ground Control Workstation PC in now mounted on Laserdyne -->
    <add key="GCWS_HOSTNAME" value="DTI-11308" />
    <!-- GCMP Ground Control Melt Pool PC in now mounted on Symbio -->
    <add key="GCMP_HOSTNAME" value="DTI-12426" />
    <!-- GCPC Ground Control (ex Laserdyne) PC viene ora utilizzato come PC di riserva -->
    <add key="GCPC_HOSTNAME" value="Unknown" />
    <!-- GCH1 Banfi Laptop -->
    <add key="GCH1_HOSTNAME" value="Unknown" />
    <!-- GCH1 Reserve -->
    <add key="GCH2_HOSTNAME" value="Unknown" />

    <!-- All data files (and not config files) generated by DSB are saved within this directory (see /docs/DSBDirStructure.vsdx) -->
    <!-- which must match with secure-file-priv set in my.ini file, otherwise errors arise while exporting config data from DB -->
    <add key="GCWS_DSB_ACQUISITION_PATH" value="I:\_GC_DataSetBuilder" />
    <add key="GCMP_DSB_ACQUISITION_PATH" value="D:\_GC_DataSetBuilder" />
    <add key="GCPC_DSB_ACQUISITION_PATH" value="D:\_GC_DataSetBuilder" />
    <add key="GCH1_DSB_ACQUISITION_PATH" value="D:\_GC_DataSetBuilder" />
    <add key="GCH2_DSB_ACQUISITION_PATH" value="D:\_GC_DataSetBuilder" />

    <!-- // Set this param to false only when Laserdyne is on AND it produces the log file -->
    <add key="GCWS_LASERDYNE_IS_ON" value="False" />
    <add key="GCMP_LASERDYNE_IS_ON" value="False" />
    <add key="GCPC_LASERDYNE_IS_ON" value="False" />
    <add key="GCH1_LASERDYNE_IS_ON" value="False" />
    <add key="GCH2_LASERDYNE_IS_ON" value="False" />
    
    <!-- Set this param to false when Symbio CN is not connected, for example when working on Laserdyne: -->
    <add key="GCWS_CN_IS_ON" value="False" />
    <add key="GCMP_CN_IS_ON" value="True" />
    <add key="GCPC_CN_IS_ON" value="False" />
    <add key="GCH1_CN_IS_ON" value="False" />
    <add key="GCH2_CN_IS_ON" value="False" />

    <!-- Set this param to false when CN is connected, but DSB does not want to listen to it, and simulate it instead: -->
    <add key="GCWS_CN_IS_SIMULATED" value="False" />
    <add key="GCMP_CN_IS_SIMULATED" value="False" />
    <add key="GCPC_CN_IS_SIMULATED" value="False" />
    <add key="GCH1_CN_IS_SIMULATED" value="False" />
    <add key="GCH2_CN_IS_SIMULATED" value="False" />

    <!-- Set this param to false when Pyrometer is not connected, for example when working on Laserdyne: -->
    <!-- Single points adjustment manually adjusted in sensorTherm app > device configuration > single point adjustment > manual adjustment: C1 = 67.1%, C2 = 47.1%, 2C = 0.0% -->
    <add key="GCWS_PYROMETER_IS_ON" value="False" />
    <add key="GCMP_PYROMETER_IS_ON" value="True" />
    <add key="GCPC_PYROMETER_IS_ON" value="False" />
    <add key="GCH1_PYROMETER_IS_ON" value="False" />
    <add key="GCH2_PYROMETER_IS_ON" value="False" />

    <!-- Set this param to false when Pyrometer is not connected, for example when working on Laserdyne: -->
    <add key="GCWS_PYROMETER_COM" value="COM1" />
    <add key="GCMP_PYROMETER_COM" value="COM3" />
    <add key="GCPC_PYROMETER_COM" value="COM1" />
    <add key="GCH1_PYROMETER_COM" value="COM1" />
    <add key="GCH2_PYROMETER_COM" value="COM1" />

    <add key="GCWS_DB_VERSION" value="gc_0_4_7_prod_gcws" />
    <add key="GCMP_DB_VERSION" value="gc_0_4_7_prod_gcmp" />
    <add key="GCPC_DB_VERSION" value="" />
    <add key="GCH1_DB_VERSION" value="False" />
    <add key="GCH2_DB_VERSION" value="False" />
    
    <!-- "IO_MODULE" means an IO device that reads for example the feeder state : -->
    <!-- "IO_MODULE" on Laserdyne is a IO device -->
    <!-- "IO_MODULE" on Symbionica is the CNCom class -->
    <add key="GCWS_IO_MODULE_IS_ON" value="False" />
    <add key="GCMP_IO_MODULE_IS_ON" value="True" />
    <add key="GCPC_IO_MODULE_IS_ON" value="False" />
    <add key="GCH1_IO_MODULE_IS_ON" value="False" />
    <add key="GCH2_IO_MODULE_IS_ON" value="False" />

    <add key="GCWS_TRIGGER_DEVICE_ENABLED" value="False" />
    <add key="GCMP_TRIGGER_DEVICE_ENABLED" value="False" />
    <add key="GCPC_TRIGGER_DEVICE_ENABLED" value="False" />
    <add key="GCH1_TRIGGER_DEVICE_ENABLED" value="False" />
    <add key="GCH2_TRIGGER_DEVICE_ENABLED" value="False" />
    
    <!-- Until here params have been entered into code: -->
    <add key="GCWS_CAMERA_NAME" value="IDS_UI-307xCP-C" />
    <add key="GCMP_CAMERA_NAME" value="IDS_UI-337xCP-C" />
    <add key="GCPC_CAMERA_NAME" value="" />
    <add key="GCH1_CAMERA_NAME" value="" />
    <add key="GCH2_CAMERA_NAME" value="" />
    <add key="GCPC_" value="False" />
    <add key="GCMP_" value="False" />
    <add key="GCWS_" value="False" />
    <add key="GCH1_" value="False" />
    <add key="GCH2_" value="False" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="OPENcontrol" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://192.168.20.2:8080" binding="basicHttpBinding" bindingConfiguration="OPENcontrol" contract="OPENcontrol_namespace.OPENcontrolPortType" name="OPENcontrol" />
    </client>
  </system.serviceModel>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>